{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryinterviews"
		},
		"adls_ls_adventurework_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls_adventurework'"
		},
		"cardata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cardata'"
		},
		"AzureMySql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySql'"
		},
		"AzureSqlDatabase_adventure_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "interviewservern.database.windows.net"
		},
		"AzureSqlDatabase_adventure_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adventureworks"
		},
		"OutpurSQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "interviewservern.database.windows.net"
		},
		"OutpurSQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "inerviewdatabase"
		},
		"adls_ls_adventurework_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsadventureworksd.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryinterviews_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_ls_adventurework_accountKey": {
						"value": "[parameters('adls_ls_adventurework_accountKey')]"
					},
					"cardata_connectionString": {
						"value": "[parameters('cardata_connectionString')]"
					},
					"AzureMySql_connectionString": {
						"value": "[parameters('AzureMySql_connectionString')]"
					},
					"AzureSqlDatabase_adventure_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_adventure_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_adventure_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_adventure_properties_typeProperties_database')]"
					},
					"OutpurSQL_properties_typeProperties_server": {
						"value": "[parameters('OutpurSQL_properties_typeProperties_server')]"
					},
					"OutpurSQL_properties_typeProperties_database": {
						"value": "[parameters('OutpurSQL_properties_typeProperties_database')]"
					},
					"adls_ls_adventurework_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_adventurework_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}